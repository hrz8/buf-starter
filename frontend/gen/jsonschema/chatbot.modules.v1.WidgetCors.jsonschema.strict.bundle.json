{
  "$defs": {
    "chatbot.modules.v1.WidgetCors.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "WidgetCors defines CORS settings for the chat widget.",
      "properties": {
        "allowedHeaders": {
          "description": "HTTP headers allowed in requests",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "allowedOrigins": {
          "description": "List of allowed origin domains for the widget",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "credentials": {
          "description": "Whether to allow credentials (cookies, authorization headers)",
          "type": "boolean"
        }
      },
      "required": [
        "credentials"
      ],
      "title": "Widget Cors",
      "type": "object"
    }
  },
  "$id": "chatbot.modules.v1.WidgetCors.jsonschema.strict.bundle.json",
  "$ref": "#/$defs/chatbot.modules.v1.WidgetCors.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
